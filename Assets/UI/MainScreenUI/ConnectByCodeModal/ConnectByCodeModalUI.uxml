					</ui:VisualElement>
		
					<ui:TextField name="codeField" label="Server code" text="" />
		
					<ui:Button name="connectButton" class="button button-lg" text="Join" />

				</ui:VisualElement>
			</ui:VisualElement>
		</ui:VisualElement>
	</ui:VisualElement>
</ui:UXML>