<?xml version="1.0" encoding="utf-8"?>
<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
	<Style src="ConnectByCodeModalStyles.uss" />

	<ui:VisualElement class="container">
		<ui:VisualElement class="row">
			<ui:VisualElement class="col col-6 align-center">
				<ui:VisualElement class="modalContainer">
					<ui:VisualElement class="modalHeader">
						<ui:Label class="h3" text="Connect to server by code" />
						 <ui:Button name="closeButton" class="iconButton transparent">
							<ui:VisualElement class="icon icon-lg closeIcon" />
